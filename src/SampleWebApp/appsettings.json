{
  "AzureAppConfiguration": {
    "AccountEndpoint": "<set-via-dotnet-user-secrets>",
    "Labels": [
      "\\0",
      "SampleWebApp"
    ],
    "Sentinels": [
      {
        "Name": "ConfigurationSentinel",
        "CacheIntervalInSeconds": 120
      },
      {
        "Name": "SampleWebApp/ConfigurationSentinel",
        "Label": "SampleWebApp",
        "CacheIntervalInSeconds": 5
      }
    ],
    "FeatureFlags": {
      "RefreshIntervalInSeconds": 2
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
